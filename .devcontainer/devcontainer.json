{
  "customizations": {
      "vscode": {
          "extensions": [
            "julialang.language-julia",
            "ms-azuretools.vscode-docker"
          ]
      }
  },
  "runArgs": [
      "--privileged"
  ],
  "dockerFile": "Dockerfile",
  "updateContentCommand": "julia -e 'using Pkg; Pkg.develop(path=\".\");'"
}